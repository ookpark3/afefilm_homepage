{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*\\.html)",
      "dest": "/admin/$1"
    },
    {
      "src": "/admin/(.*\\.(js|css|png|jpg|webp|svg))",
      "dest": "/admin/$1"
    },
    {
      "src": "/asset/work/Thumbnails/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-disposition": "inline"
      },
      "dest": "/asset/work/Thumbnails/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "dest": "/$1"
    }
  ]
}
