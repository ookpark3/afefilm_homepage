{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/asset/work/Thumbnails/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-disposition": "inline"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    }
  ]
}
